{
  "name": "vue-router-parse-props",
  "version": "0.4.0",
  "description": "Parse params and query in vue-router to component props.",
  "keywords": [
    "vue",
    "vue-router",
    "parse",
    "params",
    "query",
    "props"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "lib/**",
    "dist/**"
  ],
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "build": "unbuild",
    "version": "npm run build",
    "release": "np",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fratzinger/vue-router-parse-props.git"
  },
  "author": "Frederik Schmatz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fratzinger/vue-router-parse-props/issues"
  },
  "homepage": "https://github.com/fratzinger/vue-router-parse-props#readme",
  "peerDependencies": {
    "vue": "^3.2.0",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.6.0",
    "@types/lodash": "^4.17.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.9",
    "@vitest/coverage-v8": "^3.0.8",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "eslint": "^9.21.0",
    "mocha": "^11.1.0",
    "np": "^10.2.0",
    "prettier": "^3.5.3",
    "shx": "^0.3.4",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.8",
    "vue": "^3.5.13"
  }
}
