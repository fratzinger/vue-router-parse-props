{
  "extends": "@istanbuljs/nyc-config-typescript",
  "cache": false,
  "check-coverage": true,
  "include": [
    "src/**/*.js",
    "src/**/*.ts"
  ],
  "exclude": [
    "lib/types.ts"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary",
    "lcov"
  ],
  "sourceMap": true,
  "all": true,
  "instrument": true,
  "watermarks": {
    "statements": [70, 85],
    "lines": [70, 85],
    "functions": [70, 85],
    "branches": [70, 85]
  }
}